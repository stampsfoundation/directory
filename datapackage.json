          {
            "name": "Url",
            "type": "string",
            "format": "default"
          },
          {
            "name": "Topics",
            "type": "string",
            "format": "default"
          },
          {
            "name": "Summary",
            "type": "year",
            "format": "default"
          }
        ]
      }
    }
